JOIN RDS.DimCharterSchoolManagementOrganizations CharterSchoolUpdatedManagementOrganization
	ON Fact.CharterSchoolUpdatedManagementOrganizationId = CharterSchoolUpdatedManagementOrganization.DimCharterSchoolManagementOrganizationId
JOIN RDS.DimComprehensiveAndTargetedSupports ComprehensiveAndTargetedSupport
