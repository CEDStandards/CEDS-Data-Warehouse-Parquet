JOIN RDS.DimK12Schools K12School
	ON Fact.K12SchoolId = K12School.DimK12SchoolId
JOIN RDS.DimCharterSchoolAuthorizers CharterSchoolAuthorizer
	ON Fact.CharterSchoolAuthorizerId = CharterSchoolAuthorizer.DimCharterSchoolAuthorizerId
JOIN RDS.DimCharterSchoolManagementOrganizations CharterSchoolManagementOrganization
	ON Fact.CharterSchoolManagementOrganizationId = CharterSchoolManagementOrganization.DimCharterSchoolManagementOrganizationId
JOIN RDS.DimCharterSchoolAuthorizers CharterSchoolSecondaryAuthorizer
	ON Fact.CharterSchoolSecondaryAuthorizerId = CharterSchoolSecondaryAuthorizer.DimCharterSchoolAuthorizerId
JOIN RDS.DimCharterSchoolStatuses CharterSchoolStatus
