JOIN RDS.DimK12SchoolStatuses K12SchoolStatus
	ON Fact.K12SchoolStatusId = K12SchoolStatus.DimK12SchoolStatusId
JOIN RDS.DimK12SchoolStateStatuses K12SchoolStateStatus
	ON Fact.K12SchoolStateStatusId = K12SchoolStateStatus.DimK12SchoolStateStatusId
JOIN RDS.DimSubgroups Subgroup
	ON Fact.SubgroupId = Subgroup.DimSubgroupId
JOIN RDS.DimTitleIStatuses TitleIStatus
	ON Fact.TitleIStatusId = TitleIStatus.DimTitleIStatusId
